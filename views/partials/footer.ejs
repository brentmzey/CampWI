<!-- Custom DOM Manipulation for Navbar -->
<script>
    const navbar = document.querySelector("#mainNavbar");
    function navbarAnimate(scrollPosition) { navbar.classList.toggle("scrolled", scrollPosition > navbar.offsetHeight); };
    window.addEventListener("scroll", () => {
        let windowScrollPos = window.scrollY;
        window.requestAnimationFrame(() => { navbarAnimate(windowScrollPos) })
    });
</script>

