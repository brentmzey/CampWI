    "ejs": "^3.1.3",
    "eslint": "^7.6.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "got": "^11.5.2",
    "locus": "^2.0.4",
    "method-override": "^3.0.0",
    "moment": "^2.27.0",
    "mongoose": "^5.9.25",
    "nodemailer": "^6.4.11",
    "passport": "^0.4.1",
